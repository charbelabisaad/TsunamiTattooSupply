        .logout:hover{
           color: gainsboro;
        }
         
        .changepassword {
            color: #ffffff;
            transition: .5s;
        }

        .changepassword:hover{
            color: gainsboro;
        }

        /* Header hover */
        .user-toggle {
            background-color:#000000;
            text-decoration: none;
            transition:.5s;
            cursor: pointer;
        }

            .user-toggle:hover {
                background-color: #343a40; /* darker than bg-dark */
            }

            /* Chevron rotation */
            .user-toggle .chevron {
                transition: transform 0.25s ease;
            }

            .user-toggle[aria-expanded="true"] .chevron {
                transform: rotate(180deg);
            }

        /* Collapse background */
