        $(document).on('click', '.changepassword', function (e) {

            e.preventDefault();

            $('#ChangePasswordDestination').val('LAYOUT');

            // Reset form & errors
            $('#changePasswordForm')[0].reset();
            $('#changePasswordError').addClass('d-none').text('');

            $('#changePasswordModal').modal('show');

